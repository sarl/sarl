package io.sarl.sre.tests.runtime.bugs.to1000.bug934.mocks

import io.sarl.core.DefaultContextInteractions
import io.sarl.core.Initialize
import io.sarl.core.OpenEventSpaceSpecification
import io.sarl.sre.test.framework.skills.TestingCapacity
import io.sarl.sre.test.framework.skills.TestingSkill
import java.util.UUID

/**
 * @author $Author: sgalland$
 * @version $FullVersion$
 * @mavengroupid $GroupId$
 * @mavenartifactid $ArtifactId$
 * @see "https://github.com/sarl/sarl/issues/934"
 */
@SuppressWarnings("all")
agent WithSpecAgent {

	uses DefaultContextInteractions

	uses TestingCapacity

	on Initialize {
		setSkill(new TestingSkill(occurrence))
		var ds = defaultSpace
		ds.addResult
		var id1 = UUID::randomUUID
		id1.addResult
		var spc1 = defaultContext.getOrCreateSpaceWithSpec(typeof(OpenEventSpaceSpecification), id1)
		spc1.addResult
		var id2 = UUID::randomUUID
		id2.addResult
		var spc2 = defaultContext.getOrCreateSpaceWithSpec(typeof(OpenEventSpaceSpecification), id2)
		spc2.addResult
		killMeSoon
	}

}
