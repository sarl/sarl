package io.sarl.sre.janus.tests.skills.bic.mocks

import io.sarl.api.core.Logging
import io.sarl.lang.core.Agent
import io.sarl.lang.core.SREutils
import io.sarl.lang.core.annotation.PrivateAPI
import io.sarl.sre.janus.skills.bic.LoggingSkill
import java.util.UUID

/**
 * @author $Author: sgalland$
 * @version janus.kernel 3.0.13.0 20230919-093058
 * @mavengroupid io.sarl.sre.janus
 * @mavenartifactid janus.kernel
 */
@SuppressWarnings("all")
class MyAgent2 extends Agent {

	val logSkill : LoggingSkill

	@PrivateAPI(isCallerOnly = true)
	new (agentID : UUID, logSkill : LoggingSkill) {
		super(UUID::randomUUID, agentID)
		this.logSkill = logSkill
		SREutils::setInternalSkill(this, this.logSkill, #[typeof(Logging)])
	}
	
}
