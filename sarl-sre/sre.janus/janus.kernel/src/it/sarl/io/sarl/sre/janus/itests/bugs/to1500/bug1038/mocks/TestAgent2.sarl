package io.sarl.sre.janus.itests.bugs.to1500.bug1038.mocks

import io.sarl.api.core.ExternalContextAccess
import io.sarl.api.core.Initialize
import io.sarl.api.core.Lifecycle
import io.sarl.api.core.Schedules
import io.sarl.sre.test.framework.skills.TestingCapacity
import io.sarl.sre.test.framework.skills.TestingSkill
import java.util.UUID

/**
 * @author <a href="http://www.ciad-lab.fr/stephane_galland">St&eacute;phane Galland</a>
 * @version janus.kernel 3.0.13.0 20230919-093058
 * @mavengroupid io.sarl.sre.janus
 * @mavenartifactid janus.kernel
 */
@SuppressWarnings("all")
agent TestAgent2 {
	
	uses ExternalContextAccess, Schedules, Lifecycle
	uses TestingCapacity

	var cID : UUID
	var sID : UUID

	on Initialize {
		setSkill(new TestingSkill(occurrence))
		this.cID = getParam(0)
		this.sID = getParam(1)
		in(1.seconds) [
			var joined = join(this.cID, this.sID)
			("joined=" + joined.ID).addResult
			var contextJustJoined = getContext(this.cID)
			("context null = " + (contextJustJoined !== null)).addResult
			killMeSoon
		]
	}

}
