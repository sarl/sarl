package io.sarl.sre.janus.itests.bugs.to500.bug66.mocks

import io.sarl.api.core.DefaultContextInteractions
import io.sarl.api.core.Initialize
import io.sarl.sre.test.framework.skills.TestingCapacity
import io.sarl.sre.test.framework.skills.TestingSkill
import java.util.UUID

agent SpaceCreatorWithNamedAnnotationAgent {

	uses DefaultContextInteractions

	uses TestingCapacity

	on Initialize {
		setSkill(new TestingSkill(occurrence))
		val ctx = defaultContext
		val dftSpc = ctx.defaultSpace
		val ^space = ctx.getOrCreateSpaceWithSpec(typeof(MySpaceSpecificationWithNamedAnnotation), UUID::randomUUID)
		dftSpc.addResult
		^space.addResult
		killMeSoon
	}

}

