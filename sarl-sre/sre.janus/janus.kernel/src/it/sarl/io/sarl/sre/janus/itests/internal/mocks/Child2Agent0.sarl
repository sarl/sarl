package io.sarl.sre.janus.itests.internal.mocks

import io.sarl.api.core.ExternalContextAccess
import io.sarl.api.core.Initialize
import io.sarl.api.core.Lifecycle
import io.sarl.api.core.MemberJoined
import io.sarl.api.core.Schedules
import io.sarl.lang.core.AgentContext
import io.sarl.sre.test.framework.events.Bye
import io.sarl.sre.test.framework.skills.TestingCapacity
import io.sarl.sre.test.framework.skills.TestingSkill

import static extension io.sarl.tests.api.tools.TestUtils.*

/**
 * @author <a href="http://www.ciad-lab.fr/stephane_galland">St&eacute;phane Galland</a>
 * @version janus.kernel 3.0.14.0 20241106-161408
 * @mavengroupid io.sarl.sre.janus
 * @mavenartifactid janus.kernel
 */
agent Child2Agent0 {
	
	uses Schedules, ExternalContextAccess, Lifecycle

	uses TestingCapacity

	@SuppressWarnings("io.sarl.lang.validation.IssueCodes.potential_inefficient_value_conversion")
	on Initialize {
		setSkill(new TestingSkill(occurrence))
		val newContext = getAgentInitializationParameters.get(1) as AgentContext
		in(500) [
			if (join(newContext.ID, newContext.defaultSpace.spaceID.ID) === null) {
				addResult("CHILD2: no join")
			}
		]
	}

	on MemberJoined {
		addResult("CHILD2: joined " + occurrence.agentType.simpleTypeName)
	}

	on Bye {
		killMe
	}

}
