package io.sarl.api.core

import java.util.UUID

/** Exception thrown when an agent context is unknown.
 *
 * @author <a href="http://www.sebastianrodriguez.com.ar/">Sebastian Rodriguez</a>
 * @version api.core 0.13.0 20230919-093058
 * @mavengroupid io.sarl.sdk
 * @mavenartifactid api.core
 */
class UnknownContextException extends RuntimeException {

	val unknownContextID : UUID

	/**
	 * @param unknownContextID ID of the context that is unkown.
	 */
	new(unknownContextID : UUID) {
		this.unknownContextID = unknownContextID
	}

	/** Replies the identifier that is used to reference the unknown context.
	 *
	 * @return the unknownContextID
	 */
	def getUnknownContextID : UUID {
		this.unknownContextID
	}

}
